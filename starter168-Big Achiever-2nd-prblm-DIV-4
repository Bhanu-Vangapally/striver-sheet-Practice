# cook your dish here
for _ in range(int(input())):
    n=int(input())
    l=list(map(int,input().split()))
    mx=l[0]
    ans=[]
    ans.append(1)
    for i in range(1,len(l)):
        if(l[i]>mx):
            mx=l[i]
            ans.append(1)
        else:
            ans.append(0)
    print(*ans)